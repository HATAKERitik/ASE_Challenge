generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      ="file:./dev.db"
}

model products{
  id Int @id @default(autoincrement())
  Name String
  Description String
  stock_quntity Int
  updateDate DateTime @updatedAt
}